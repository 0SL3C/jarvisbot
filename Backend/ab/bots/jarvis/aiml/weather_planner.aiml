<?xml version="1.0" encoding="UTF-8"?>
<aiml>
    <category>
        <pattern><SET>CITIES</SET></pattern>
        <template><star/></template>
    </category>
    <category>
        <pattern>HAVE <SET>CITIES</SET></pattern>
        <template><star/></template>
    </category>
    <category>
        <pattern>WEATHER TEST</pattern>
        <template>Weather planner is working correctly!</template>
    </category>
    <category>
        <pattern>WHAT IS * PLUS *</pattern>
        <template><srai>CALCULATE <star index="1"/> + <star index="2"/></srai></template>
    </category>
    <category>
        <pattern>WHAT IS * MINUS *</pattern>
        <template><srai>CALCULATE <star index="1"/> - <star index="2"/></srai></template>
    </category>
    <category>
        <pattern>WHAT IS * TIMES *</pattern>
        <template><srai>CALCULATE <star index="1"/> * <star index="2"/></srai></template>
    </category>
    <category>
        <pattern>WHAT IS * DIVIDED BY *</pattern>
        <template><srai>CALCULATE <star index="1"/> / <star index="2"/></srai></template>
    </category>
    <category>
        <pattern>CALCULATE * * *</pattern>
        <template>
            <think>
                <set name="num1"><star index="1"/></set>
                <set name="operator"><star index="2"/></set>
                <set name="num2"><star index="3"/></set>
            </think>
            <srai>PERFORMCALC</srai>
        </template>
    </category>
    <category>
        <pattern>PERFORMCALC</pattern>
        <template>CALCULATION_RESULT</template>
    </category>
</aiml>